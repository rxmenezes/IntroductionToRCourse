pulse <- read.delim("data/pulse.txt")
pulse$exercise <- as.factor(pulse$exercise)

pulse <- read.delim("data/pulse.txt", stringsAsFactors = TRUE)


pulse$exercise <- factor(pulse$exercise, levels=c('high', 'moderate', 'low'))
table(pulse$exercise, pulse$exercise2)

form <- weights ~ alcohol
boxplot(form, data = pulse)
t.test(form, data = pulse)

addone <- function(x){
  x + 1
}

tb <- function(form1){
  mybx <- boxplot(form1, data = pulse)
  tp <- t.test(form1, data = pulse)$p.value
  return(list(tp, mybx$stats[1])
}
